doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
  body 
    - const links = ["colorsAndType", "formElements", "headersAndFooters"];
    each link in links
      li
        a(href=`./${link}.html`)= link
